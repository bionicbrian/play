body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF
  text-decoration: none

.navigator
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 40px
    color: #888
    font-size: 11pt
    line-height: 40px
    padding: 0 20px
    background-color: #202020

    a
    a:visited
    a:active
        color: #888
        text-decoration: none

    a:hover
        color: white
        text-decoration: none
